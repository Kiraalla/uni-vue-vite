{
  "easycom": {
    "autoscan": true,
    "custom": {
      "^z-(.*)": "zebra-swiper/components/z-$1/z-$1.vue"
    }
  },
  "pages": [
    {
      "path": "pages/index/index",
      "style": {
        "navigationBarTitleText": "uni-app"
      }
    },
    {
      "path": "pages/demo/http-test",
      "style": {
        "navigationBarTitleText": "HTTP工具测试"
      }
    },
    {
      "path": "pages/components/index",
      "style": {
        "navigationBarTitleText": "组件展示"
      }
    },
    {
      "path": "pages/components/detail",
      "style": {
        "navigationBarTitleText": "组件详情",
        "navigationStyle": "custom"
      }
    }
  ],
  "globalStyle": {
    "navigationBarTextStyle": "black",
    "navigationBarTitleText": "uni-app",
    "navigationBarBackgroundColor": "#F8F8F8",
    "backgroundColor": "#F8F8F8"
  }
}